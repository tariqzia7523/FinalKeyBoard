<?xml version="1.0" encoding="utf-8"?>


<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <!-- The keys on this PC layout row4 consist of the letters of QWERTY layout row3 and
         some symbols keys. -->
    <include
        latin:keyboardLayout="@xml/rowkeys_qwerty3" />
